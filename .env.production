# Production Environment Variables for app.trynashr.com

# Salla OAuth Configuration
SALLA_OAUTH_CLIENT_ID=your_salla_client_id_here
SALLA_OAUTH_CLIENT_SECRET=your_salla_client_secret_here
SALLA_OAUTH_CLIENT_REDIRECT_URI=https://app.trynashr.com/oauth/callback

# Webhook Configuration
SALLA_WEBHOOK_SECRET=your_webhook_secret_here

# App Configuration
NODE_ENV=production
SESSION_SECRET=super_secure_session_secret_change_this
PORT=3000

# Database (optional - currently using in-memory storage)
# DATABASE_URL=mongodb://localhost:27017/miqasi
# DATABASE_URL=mysql://user:pass@localhost:3306/miqasi

# Analytics (optional)
# GOOGLE_ANALYTICS_ID=GA-XXXXX-X
# MIXPANEL_TOKEN=your_mixpanel_token

# Logging
LOG_LEVEL=info