{
  "functions": {
    "public/store-widget.js": {
      "runtime": "@vercel/static"
    }
  },
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/api/dashboard"
    },
    {
      "source": "/widget.js",
      "destination": "/public/store-widget.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/api/dashboard"
    }
  ]
}