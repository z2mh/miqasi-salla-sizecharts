{
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/api/dashboard"
    },
    {
      "source": "/widget.js",
      "destination": "/public/store-widget.js"
    },
    {
      "source": "/",
      "destination": "/api/dashboard"
    }
  ]
}